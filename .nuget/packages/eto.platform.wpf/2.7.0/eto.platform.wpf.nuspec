    <tags>cross platform gui ui framework desktop wpf eto.forms</tags>
    <repository type="git" url="https://github.com/picoe/Eto.git" />
    <dependencies>
      <group targetFramework=".NETFramework4.6.2">
        <dependency id="Eto.Forms" version="2.7.0" exclude="Build,Analyzers" />
        <dependency id="Extended.Wpf.Toolkit" version="3.6.0" exclude="Build,Analyzers" />
        <dependency id="Microsoft-WindowsAPICodePack-Shell" version="1.1.4" exclude="Build,Analyzers" />
      </group>
      <group targetFramework=".NETCoreApp3.1">
        <dependency id="Eto.Forms" version="2.7.0" exclude="Build,Analyzers" />
        <dependency id="Extended.Wpf.Toolkit" version="3.6.0" exclude="Build,Analyzers" />
        <dependency id="Microsoft-WindowsAPICodePack-Shell" version="1.1.4" exclude="Build,Analyzers" />
      </group>
    </dependencies>
    <frameworkReferences>
      <group targetFramework=".NETCoreApp3.1">
        <frameworkReference name="Microsoft.WindowsDesktop.App" />
      </group>
      <group targetFramework=".NETFramework4.6.2" />
    </frameworkReferences>
  </metadata>
</package>